# Copy this file to .env and fill in the values.

# API key for the Python backend (required)
KVR_API_KEY=your_api_key_here

# Backend URL (optional, defaults to http://localhost:5000)
KVR_BACKEND_URL=http://localhost:5000

# Frontend port (optional)
FRONTEND_PORT=3000

# Session secret for express-session (change in production)
SESSION_SECRET=change_this_secret
